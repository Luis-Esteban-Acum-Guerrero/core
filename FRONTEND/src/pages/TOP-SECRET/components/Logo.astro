---
const { emisor } = Astro.props;
---

<div class="logo-container">
  <img src={`/assets/${emisor.logo}`} alt={emisor.nombre} />
</div>

<style>
  .logo-container {
    width: 100%;
    margin-bottom: 24px;
    margin-top: 100px;
    text-align: right;
  }

  .logo-container img {
    width: 100px;
    height: auto;
    /*filter: grayscale(100%);*/
  }
</style>
