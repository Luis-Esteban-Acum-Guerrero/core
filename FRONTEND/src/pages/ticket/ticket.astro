---
import TicketMenu from "../../components/ticket/TicketMenu.astro";
import TicketDetail from "../../components/ticket/TicketDetail.astro";

// GET TICKETS from JSON
import { getRandomTickets } from "../../../public/data/DBTicket.js";
const tickets = getRandomTickets(Math.floor(Math.random() * 10) + 6);
---

<main
  id="content"
  class="2xl:hs-overlay-layout-open:pe-96 lg:ps-80 xl:ps-160 transition-all duration-300"
>
  <TicketMenu tickets={tickets} />
  <TicketDetail tickets={tickets} />
</main>

